{
    "name": "vinelab/neoeloquent",
    "description": "Laravel wrapper for the Neo4j graph database REST interface",
    "keywords": [
        "neo4j",
        "ogm",
        "graph",
        "database",
        "neoeloquent"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Abed Halawi",
            "email": "abed.halawi@vinelab.com"
        },
        {
            "name": "Kinane Domloje",
            "email": "kinane@vinelab.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "nesbot/carbon": "^2.0",
        "laudis/neo4j-php-client": "^2.4.2",
        "wikibase-solutions/php-cypher-dsl": "dev-main",
        "psr/container": "^1.0",
        "illuminate/contracts": "^8.0",
        "stefanak-michal/bolt": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.3.0",
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "*",
        "fzaninotto/faker": "~1.4",
        "composer/composer": "^2.1",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Vinelab\\NeoEloquent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinelab\\NeoEloquent\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Vinelab\\NeoEloquent\\NeoEloquentServiceProvider"
            ]
        }
    }
}
