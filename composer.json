{
    "name": "vinelab/neoeloquent",
    "description": "Laravel wrapper for the Neo4j graph database REST interface",
    "keywords": [
        "neo4j",
        "ogm",
        "graph",
        "database",
        "neoeloquent"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Abed Halawi",
            "email": "abed.halawi@vinelab.com"
        },
        {
            "name": "Kinane Domloje",
            "email": "kinane@vinelab.com"
        },
        {
            "name": "Ghlen Nagels",
            "email": "ghlen@pm.me"
        }
    ],
    "require": {
        "php": "^8.1",
        "laudis/neo4j-php-client": "^3.0.1",
        "psr/container": "^1.1.2",
        "illuminate/contracts": "^10.0",
        "illuminate/database": "^10.0",
        "wikibase-solutions/php-cypher-dsl": "^5.0",
        "php-graph-group/cypher-query-builder": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0.19",
        "laravel/pint": "^1.8",
        "vimeo/psalm": "^5.9",
        "psalm/plugin-laravel": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "Vinelab\\NeoEloquent\\": "src/",
            "PhpGraphGroup\\QueryBuilder\\": "query-builder/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinelab\\NeoEloquent\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Vinelab\\NeoEloquent\\NeoEloquentServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
