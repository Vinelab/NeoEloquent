{
    "name": "vinelab/neoeloquent",
    "description": "Laravel wrapper for the Neo4j graph database REST interface",
    "keywords": [
        "neo4j",
        "ogm",
        "graph",
        "database",
        "neoeloquent"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Abed Halawi",
            "email": "abed.halawi@vinelab.com"
        },
        {
            "name": "Kinane Domloje",
            "email": "kinane@vinelab.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "nesbot/carbon": "^2.0",
        "laudis/neo4j-php-client": "^3.0",
        "psr/container": "^1.0",
        "illuminate/contracts": "^10.0",
        "wikibase-solutions/php-cypher-dsl": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.0"
    },
    "repositories": [
        {
            "url": "https://github.com/transistive/php-cypher-dsl.git",
            "type": "git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Vinelab\\NeoEloquent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinelab\\NeoEloquent\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Vinelab\\NeoEloquent\\NeoEloquentServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
